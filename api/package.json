{
  "name": "todo-list",
  "version": "1.0.0",
  "description": "",
  "main": "build/app/index.js",
  "scripts": {
    "build": "npx -y tsc",
    "generate:salt": "node ./app/utils/test",
    "db:drop": "npx sequelize-cli db:drop",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:restart": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "dev": "NODE_ENV=development npm run db:restart && node .",
    "prestart": "npm run build && npm run db:restart",
    "pretest": "NODE_ENV=test npm run db:restart",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "joi": "^17.6.0",
    "mysql2": "^2.3.3",
    "sequelize": "^6.19.0"
  },
  "devDependencies": {
    "@tsconfig/node17": "^1.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.34",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0"
  }
}
